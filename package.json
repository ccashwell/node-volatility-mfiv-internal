{
  "name": "@volatility-group/node-volatility-mfiv",
  "version": "0.2.3-alpha.1",
  "description": "Library for computing and validating model-free implied volatility (MFIV).",
  "repository": "github:VolatilityGroup/node-volatility-mfiv-internal.git",
  "bugs": "https://github.com/VolatilityGroup/node-volatility-mfiv-internal/issues",
  "main": "./dist/src/index.js",
  "source": "src/index.ts",
  "types": "./dist/src/index.d.ts",
  "homepage": "https://github.com/VolatilityGroup/node-volatility-mfiv#README.md",
  "scripts": {
    "release": "standard-version",
    "release:minor": "standard-version --release-as minor",
    "release:patch": "standard-version --release-as patch",
    "release:major": "standard-version --release-as major",
    "build": "tsc -p tsconfig-build.json",
    "docs": "npx typedoc src/index.ts",
    "outdated": "ncu",
    "outdated:upgrade": "ncu -u && npm install",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint src --fix",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'test/**/*.spec.ts'",
    "test:unit": "mocha",
    "test:coverage": "nyc npm run test",
    "watch": "nodemon --exec ./node_modules/.bin/ts-node ./src/index.ts",
    "prepare": "husky install",
    "volatility-check": "./bin/volatility.mjs check -e",
    "example": "npm run volatility-check eth-mfiv-14d-2021-10-01T07:02:00.000Z",
    "vulnerabilities": "npm audit"
  },
  "bin": {
    "volatility-check": "bin/volatility.mjs"
  },
  "files": [
    "dist",
    "bin"
  ],
  "engines": {
    "node": ">=14.18.1 <17",
    "npm": ">=8.3.1 <9"
  },
  "keywords": [
    "volatility",
    "mfiv",
    "model free implied volatility",
    "ethereum",
    "crypto",
    "volatility index",
    "ethVOL"
  ],
  "author": "nick@volatility.com",
  "os": [
    "darwin",
    "linux"
  ],
  "license": "MIT",
  "devDependencies": {
    "@istanbuljs/nyc-config-typescript": "^1.0.2",
    "@types/chai": "^4.3.0",
    "@types/chai-as-promised": "^7.1.5",
    "@types/debug": "^4.1.7",
    "@types/lodash": "^4.14.180",
    "@types/mocha": "^9.1.0",
    "@types/node": "^17.0.10",
    "@typescript-eslint/eslint-plugin": "^5.15.0",
    "@typescript-eslint/parser": "^5.15.0",
    "chai": "^4.3.6",
    "chai-as-promised": "^7.1.1",
    "eslint": "^8.11.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^2.5.0",
    "eslint-plugin-import": "^2.25.4",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-promise": "^6.0.0",
    "husky": "^7.0.4",
    "mocha": "^9.2.2",
    "nodemon": "^2.0.15",
    "nyc": "^15.1.0",
    "prettier": "^2.6.0",
    "standard-version": "^9.3.2",
    "ts-node": "^10.7.0",
    "typedoc": "^0.22.13",
    "typescript": "^4.6.2"
  },
  "dependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@nozzlegear/railway": "^2.0.0",
    "chalk": "^5.0.1",
    "dayjs": "^1.11.0",
    "debug": "^4.3.4",
    "lodash": "^4.17.21",
    "node-emoji": "^1.11.0",
    "transducist": "^2.2.0",
    "tslib": "^2.3.1",
    "uuid": "^8.3.2",
    "yargs": "^17.4.0"
  },
  "overrides": {
    "nyc": {
      "istanbul-lib-processinfo": {
        "uuid": "$uuid"
      }
    },
    "ts-node": {
      "uuid": "$uuid"
    }
  },
  "nyc": {
    "extends": "@istanbuljs/nyc-config-typescript",
    "check-coverage": true,
    "all": true,
    "include": [
      "src/**/!(*.test.*).[tj]s?(x)"
    ],
    "exclude": [
      "src/_tests_/**/*.*"
    ],
    "reporter": [
      "html",
      "lcov",
      "text",
      "text-summary"
    ],
    "report-dir": "coverage"
  },
  "lint-staged": {
    "*.{ts}": [
      "eslint",
      "prettier --write",
      "git add"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  }
}
